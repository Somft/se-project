@model ExBook.Models.UsersViewModel

<h2>
    Users
</h2>

<table>
    <tr>
        <th>ID</th>
        <th>Login</th>
        <th>Is administrator</th>
    </tr>
    @foreach (var user in Model.Users)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Login</td>
            <td>@user.IsAdministrator</td>
        </tr>
    }
</table>